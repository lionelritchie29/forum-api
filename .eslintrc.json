{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "jest/globals": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "no-underscore-dangle": "off",
    "class-methods-use-this": "off"
  },
  "plugins": ["jest"]
}
